{
  "compilerOptions": {
    "target": "es6",
    "module": "es6", 
    "outDir": "./",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "~/*": [
        "./src/*"
      ],
      "react-native/*": [
        "./node_modules/react-native-web/*"
      ]
    }
  },
  "extends": "expo-module-scripts/tsconfig.base",
  "include": ["./src"],
  "exclude": ["**/__mocks__/*", "**/__tests__/*", "examples", "node_modules"]
}